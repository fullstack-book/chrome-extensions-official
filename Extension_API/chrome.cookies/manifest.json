{
    "manifest_version": 3,
    "name": "chrome.cookies",
    "description": "chrome.cookies API 基础用法",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "cookies"
    ]
}