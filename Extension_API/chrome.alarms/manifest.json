{
    "manifest_version": 3,
    "name": "chrome.alarms",
    "description": "chrome.alarms API 基础用法",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon128.png"
    },
    "permissions": [
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    }
}